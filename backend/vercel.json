{
  "version": 2,
  "builds": [
    {
      "src": "./Server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Server.js"
    },
    {
      "src": "./Uploads/ArticleImg/(.*)",
      "dest": "/api/Article/ArticleImg/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Server.js"
    }
  ]
}
